{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs/swagger.json",
      "dest": "dist/swagger.json"
    },
    {
      "src": "/(.*)",
      "dest": "dist/src/server.js"
    }
  ]
}